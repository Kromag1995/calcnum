\begin_layout Subsection
Punto Fijo
\end_layout

\begin_layout Standard
\begin_inset Formula $f\left(x\right)=x^{3}-13x+18$
\end_inset


\end_layout

\begin_layout Standard
Queremos resolver 
\begin_inset Formula $f\left(x\right)=0$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $x^{3}-13x+18=0\rightarrow$
\end_inset


\begin_inset Formula $x^{3}+18=13x\rightarrow x=\frac{x^{3}+18}{13}=g\left(x\right)$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $x^{3}-13x+18=0\rightarrow x^{3}-12x-x+18=0\rightarrow x^{3}-12x+18=x$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $x^{3}-13x+18=0\rightarrow xx^{2}=13x-18\rightarrow x=\frac{13x-18}{x^{2}}$
\end_inset


\end_layout

\begin_layout Standard
Se define una sucesion por iteracion, elige 
\begin_inset Formula $x_{0}$
\end_inset

 y 
\begin_inset Formula $x_{n+1}=g\left(x_{n}\right)$
\end_inset

.
 Si converge es a un punto fijo.
\end_layout

\begin_layout Standard

\series bold
Teo:
\series default
 I=
\begin_inset Formula $\left[a,b\right]$
\end_inset

 si 
\begin_inset Formula $g\left(I\right)\subset I\rightarrow g$
\end_inset

 tiene al menos 1 punto fijo.
\end_layout

\begin_layout Standard

\series bold
Teo: 
\series default
Si g es derivable y 
\begin_inset Formula $\left|g'\left(x\right)\right|\leq\lambda<1\forall x\epsilon I$
\end_inset

 y 
\begin_inset Formula $g\left(I\right)\subset I\rightarrow$
\end_inset

tiene unico punto fijo.
\end_layout

\begin_layout Standard

\series bold
Teo:
\series default
 
\begin_inset Formula $g/\;\left|g'\left(x\right)\right|\leq\lambda<1\forall x\epsilon I$
\end_inset

 y 
\begin_inset Formula $g\left(I\right)\subset I\rightarrow\left(x_{n}\right)$
\end_inset

 def 
\begin_inset Formula $x_{n+1}=g\left(x_{n}\right)$
\end_inset

converge al unico punto y
\end_layout

\begin_layout Standard
1) 
\begin_inset Formula $\left|x_{n}-r\right|\leq\lambda^{n}\left|x_{o}-r\right|$
\end_inset


\end_layout

\begin_layout Standard
2) 
\begin_inset Formula $\left|e_{n}\right|\leq\frac{\lambda^{n}}{1-\lambda}\left|x_{1}-x_{0}\right|$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Teo:
\series default
 y continua en 
\begin_inset Formula $\left(a,b\right)$
\end_inset

, 
\begin_inset Formula $r\epsilon\left(a,b\right)$
\end_inset

, punto fijo de g.
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\left|g'\left(r\right)\right|<1\rightarrow\exists\epsilon>0\;/$
\end_inset

 la iteracion es convergente si 
\begin_inset Formula $x_{0}\epsilon I_{\varepsilon}=\left(r-\varepsilon,r+\varepsilon\right)$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Ejemplo:
\end_layout

