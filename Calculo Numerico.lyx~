\end_layout

\begin_layout Standard
Dar una discretizacion para el problema:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
u''+u=0\\
u\left(0\right)=A & u\left(\frac{\pi}{2}\right)=B
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
Donde A y B son parametros del problema.
\end_layout

\begin_layout Standard
Discretizamos 
\begin_inset Formula $\left[0,\frac{\pi}{2}\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i}=i\left(\frac{\pi}{2M}\right),0\leq i\leq M$
\end_inset


\end_layout

\begin_layout Standard
Voy a aproximar 
\begin_inset Formula $u''\left(x_{i}\right)+u\left(x_{i}\right)=0$
\end_inset

 en el interior y en los contornos uso las condiciones, A y B.
\end_layout

\begin_layout Standard
Llamamos a 
\begin_inset Formula $u_{i}$
\end_inset

a la aproximacion
\end_layout

\begin_layout Standard
De 
\begin_inset Formula $u\left(x_{i}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u''\left(x_{i}\right)\sim\frac{u_{i+1}-2u_{i}+u_{i-1}}{h^{2}},1\leq i\leq M-1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
\frac{u_{i+1}-2u_{i}+u_{i-1}}{h^{2}}+u_{i}=0 & 1\leq i\leq M-1\\
u_{0}=A,u_{M}=B & Bordes
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{i+1}-2u_{i}+u_{i-1}+h^{2}u_{i}=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{i+1}-\left(2-h^{2}\right)u_{i}+u_{i-1}=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{i+1}+\alpha\left(h\right)u_{i}+u_{i-1}=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{cccccccc}
1 & 0 & \ldots & \ldots &  &  & \ldots & 0\\
1 & \alpha\left(h\right) & 1 & \ldots &  &  & \ldots & 0\\
0 & 1 & \alpha\left(h\right) & 1 &  &  & \ldots & \vdots\\
\vdots & \vdots & \vdots &  & \ldots & \ldots & \ldots & 0\\
0 & 0 & \ldots & 0 & \ldots & \ldots & 0 & 1
\end{array}\right)\left(\begin{array}{c}
u_{0}\\
\vdots\\
\vdots\\
\vdots\\
u_{m}
\end{array}\right)=\left(\begin{array}{c}
A\\
\vdots\\
\vdots\\
\vdots\\
B
\end{array}\right)$
\end_inset


\end_layout

