\begin_layout Section
03/09
\end_layout

\begin_layout Subsection
Sistema de n ecuaciones.
\end_layout

\begin_layout Standard
¿Cuanto afectará a la solucion un error en el dato b?¿Como medir el error?
\end_layout

\begin_layout Standard
Dado 
\begin_inset Formula $n\epsilon\mathbb{N}$
\end_inset

, tenemos definidas 
\begin_inset Formula $\left|\left|.\right|\right|_{1},\left|\left|.\right|\right|_{2}...\left|\left|.\right|\right|_{\infty}$
\end_inset


\end_layout

\begin_layout Standard
Para todo 
\begin_inset Formula $x\epsilon\mathbb{R}^{n}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{array}{c}
\left|\left|x\right|\right|_{1}=\mathop{\varSigma\left|x_{i}\right|}\\
\left|\left|x\right|\right|_{\infty}=max\left|x_{i}\right|\\
\left|\left|x\right|\right|_{2}=\sqrt{\Sigma x_{i}^{2}}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
Hay una equivalencia entre las normas!!
\end_layout

\begin_layout Standard

\series bold
Por ejemplo
\series default
 
\begin_inset Formula $\left|\left|x\right|\right|_{\infty}\leq\left|\left|x\right|\right|_{1}\leq n\left|\left|x\right|\right|_{\infty}$
\end_inset


\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|x\right|\right|_{1}\leq n\left|\left|x\right|\right|_{\infty}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|x\right|\right|_{1}=\left|x_{1}\right|+...\left|x_{n}\right|\leq max\left|x_{i}\right|+max\left|x_{i}\right|+...+max\left|x_{i}\right|=\left|\left|x\right|\right|_{\infty}+....+\left|\left|x\right|\right|_{\infty}=n\left|\left|x\right|\right|_{\infty}$
\end_inset


\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|x\right|\right|_{\infty}\leq\left|\left|x\right|\right|_{1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|x\right|\right|_{\infty}=max\left|x_{i}\right|\leq\left|x_{1}\right|+\left|x_{2}\right|+...+\left|x_{n}\right|=\left|\left|x\right|\right|_{1}$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Otro ejemplo
\series default
 
\begin_inset Formula $\left|\left|x\right|\right|_{2}\leq\left|\left|x\right|\right|_{1}\leq\sqrt{n}\left|\left|x\right|\right|_{2}$
\end_inset


\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|x\right|\right|_{2}^{2}\leq\left|\left|x\right|\right|_{1}^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|x\right|\right|_{2}^{2}=x_{1}^{2}+...+x_{n}^{2}=\left|x_{1}\right|^{2}+...+\left|x_{n}\right|^{2}\leq\left(\left|x_{1}\right|+...+\left|x_{n}\right|\right)^{2}=\left|\left|x\right|\right|_{1}^{2}$
\end_inset


\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|x\right|\right|_{1}\leq\sqrt{n}\left|\left|x\right|\right|_{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|x\right|\right|_{1}^{2}\leq n\left|\left|x\right|\right|_{2}^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|x\right|\right|_{1}^{2}=\left(\left|x_{1}\right|+...+\left|x_{n}\right|\right)^{2}\leq n\left(x_{1}^{2}+...+x_{n}^{2}\right)$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Desilgualdad de C-S:
\end_layout

\begin_layout Standard
Para todo 
\begin_inset Formula $a,b\in\mathbb{R}^{n}$
\end_inset

se tiene que:
\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\overline{a}\overline{b}\right)^{2}\leq\left(\overline{a}\overline{a}\right).\left(\overline{b}\overline{b}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\left(\left|x_{1}\right|,...,\left|x_{n}\right|\right).\left(1,...,1\right)\right)^{2}\leq$
\end_inset


\begin_inset Formula $\left(\left(\left|x_{1}\right|,...,\left|x_{n}\right|\right).\left(\left|x_{1}\right|,...,\left|x_{n}\right|\right)\right)\left(\left(1,...,1\right).\left(1,...,1\right)\right)$
\end_inset


\end_layout

\begin_layout Standard

\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\left|x_{1}\right|+...+\left|x_{n}\right|\right)^{2}\leq\left(x_{1}^{2}+...+x_{n}^{2}\right)n$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Ejemplo
\series default
 Queremos resolver el siguiente sistema 
\begin_inset Formula $Ax=b$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{cc}
4.1 & 2.8\\
9.7 & 6.6
\end{array}\right)\left(\begin{array}{c}
x_{1}\\
x_{2}
\end{array}\right)=\left(\begin{array}{c}
4.1\\
9.7
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
Modificamos 'b' 
\begin_inset Formula $\vec{A}\vec{x}=\vec{b}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{cc}
4.1 & 2.8\\
9.7 & 6.6
\end{array}\right)\left(\begin{array}{c}
x_{1}\\
x_{2}
\end{array}\right)=\left(\begin{array}{c}
4.11\\
9.7
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|\Delta b\right|\right|_{1}=\left|\left|b-\vec{b}\right|\right|_{1}=\left|\left|\left(\begin{array}{c}
-0.01\\
0
\end{array}\right)\right|\right|_{1}=\left|-0.01\right|+\left|0\right|=0.01$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|\Delta x\right|\right|_{1}=\left|\left|x-\vec{x}\right|\right|_{1}=\left|\left|\left(\begin{array}{c}
0.66\\
-0.97
\end{array}\right)\right|\right|_{1}=\left|0.66\right|+\left|0.97\right|=1.63$
\end_inset

 
\end_layout

\begin_layout Standard
El error relativo se amplifica mucho
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\left|\left|x-\vec{x}\right|\right|_{1}}{\left|\left|x\right|\right|_{1}}=\frac{1.63}{1}=1.63$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\left|\left|b-\vec{b}\right|\right|_{1}}{\left|\left|b\right|\right|_{1}}=\frac{0.01}{13.8}=0.724*10^{-3}$
\end_inset


\end_layout

\begin_layout Standard
¿Por que este comportamiento?
\end_layout

\begin_layout Standard
Dado A ¿Cual sera el factor de amplificacion del error relativo? ¿Tiene
 cota?
\end_layout

\begin_layout Standard
Para una matriz arbitraria 
\begin_inset Formula $A\in\mathbb{R}^{nxn}$
\end_inset

y una norma de vectores 
\begin_inset Formula $\left|\left|\right|\right|$
\end_inset

cualquiera, definimos la norma matricial asociada como:
\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|=max\frac{\left|\left|Ax\right|\right|}{\left|\left|x\right|\right|}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|=max\left|\left|Ax\right|\right|$
\end_inset

 tomando x con 
\begin_inset Formula $\left|\left|x\right|\right|=1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|_{1}=\max_{j}\sum_{i=1}^{n}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}=\max_{i}\sum_{j=i}^{n}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
Ahora lo vamos a demostrar
\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}=\max_{i}\sum_{j=1}^{n}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|Ax\right|\right|_{\infty}=\left|\left|\begin{array}{c}
a_{11}x+...+a_{1m}x_{m}\\
a_{m1}x_{1}+...+a_{mm}x_{m}
\end{array}\right|\right|_{\infty}=max\left\{ \left|a_{11}x+...+a_{1m}x_{m}\right|,...,\left|a_{m1}x_{1}+...+a_{mm}x_{m}\right|\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $max_{i}\left|\sum_{j=1}^{m}a_{ij}x_{j}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|\left|x_{j}\right|\leq max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|\left|\left|x\right|\right|_{\infty}=\left|\left|x\right|\right|_{\infty}max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|Ax\right|\right|_{\infty}\leq\left|\left|x\right|\right|_{\infty}max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\left|\left|Ax\right|\right|_{\infty}}{\left|\left|x\right|\right|_{\infty}}\leq max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}\leq max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
Queda demostrado.
 Ahora vamos para la otra desigualdad
\end_layout

\begin_layout Standard
qvq 
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}\geq max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $k$
\end_inset

 tal que 
\begin_inset Formula $\sum_{j=1}^{m}\left|a_{kj}\right|$
\end_inset

 es maximo y tomemos: 
\begin_inset Formula $x=\left(\begin{array}{c}
sg\left(a_{k1}\right)\\
...\\
sg\left(a_{km}\right)
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(Ax\right)_{k}=\left(\begin{array}{ccc}
a_{11} & ... & a_{1m}\\
... & ... & ...\\
a_{k1} & ... & a_{km}\\
... & ... & ...\\
a_{mm} & ... & a_{mm}
\end{array}\right)\left(\begin{array}{c}
sg\left(a_{11}\right)\\
...\\
sg\left(a_{k1}\right)\\
...\\
sg\left(a_{mm}\right)
\end{array}\right)=\sum_{j=1}^{m}\left|a_{kj}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|Ax\right|\right|_{\infty}\geq\sum_{j=1}^{m}\left|a_{kj}\right|$
\end_inset

 y como 
\begin_inset Formula $\left|\left|x\right|\right|_{\infty}=1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\left|\left|Ax\right|\right|_{\infty}}{\left|\left|x\right|\right|_{\infty}}\geq\sum_{j=1}^{m}\left|a_{kj}\right|$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}\geq\frac{\left|\left|Ax\right|\right|_{\infty}}{\left|\left|x\right|\right|_{\infty}}\geq max_{i}\sum_{j=1}^{m}\left|a_{ij}\right|$
\end_inset


\end_layout

