\begin_layout Subsection
Eliminacion Gaussiana
\end_layout

\begin_layout Standard
\begin_inset Formula $A\epsilon\Re^{nxn},\,b\epsilon\Re^{nxn}$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $Ax=b$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(A|b\right)\rightarrow\left(\vec{A}|\vec{b}\right)$
\end_inset


\end_layout

\begin_layout Standard
Ejemplo:
\end_layout

\begin_layout Standard
\begin_inset Formula $A=\left(\begin{array}{ccc}
1 & 1 & 1\\
2 & 0 & -1\\
3 & 1 & 2
\end{array}\right)\,B=\left(\begin{array}{c}
1\\
0\\
-1
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{ccc}
1 & 1 & 1\\
2 & 0 & -1\\
3 & 1 & 2
\end{array}\begin{array}{c}
|\\
|\\
|
\end{array}\begin{array}{c}
1\\
0\\
-1
\end{array}\right)\rightarrow\left(\begin{array}{ccc}
1 & 1 & 1\\
0 & -2 & -3\\
0 & -2 & -1
\end{array}\begin{array}{c}
|\\
|\\
|
\end{array}\begin{array}{c}
1\\
-2\\
-4
\end{array}\right)\rightarrow\left(\begin{array}{ccc}
