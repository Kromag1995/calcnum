\end_inset


\end_layout

\begin_layout Paragraph
Uso Practico
\end_layout

\begin_layout Standard
\begin_inset Formula $I=\left[a,b\right],u:I\rightarrow R$
\end_inset


\end_layout

\begin_layout Standard
