\begin_layout Subsection
Metodos implicitos/explicitos
\end_layout

\begin_layout Standard
\begin_inset Formula $\left\{ \begin{array}{c}
x'=f\left(t,x\right)\\
x\left(t_{0}\right)=x_{0}
\end{array}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $t_{i}=t_{o}+i\left(\frac{T-t_{0}}{M}\right)$
\end_inset

, 
\begin_inset Formula $k=\frac{T-t_{0}}{M}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i}\thicksim x\left(t_{i}\right)$
\end_inset


\end_layout

\begin_layout Subsubsection
Metodos Explicitos (de un paso)
\end_layout

\begin_layout Standard
