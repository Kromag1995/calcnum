\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i}-x_{i-1}=h,i=1,\ldots,u$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{i}=u\left(x_{i}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u_{i+1}=u\left(x_{i+1}\right)=u\left(x_{i}+h\right)$
\end_inset


\end_layout

\begin_layout Standard
