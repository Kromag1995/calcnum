\begin_layout Standard
\begin_inset Formula $\begin{array}{c}
\overline{x}'=f\left(t,\overline{x}\right),t_{0}\leq t\leq\tau\\
\overline{x}\left(t_{0}\right)=x_{0}
\end{array}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\begin{array}{c}
x'=ax+by\\
y'=cx+dy\\
x\left(t_{0}\right)=x_{0}\\
y\left(t_{0}\right)=y_{0}
\end{array}$
\end_inset


\end_layout

\begin_layout Standard
Euler 
\begin_inset Formula $\overline{x}_{i+1}=\overline{x}_{i}+hf\left(t_{i},\overline{x}_{i}\right)$
\end_inset


\end_layout

\begin_layout Subsubsection
Octave
\end_layout

\begin_layout Standard
function [t,x,y]=euler2x2(a,b,c,d,t0,T,x0,y0,M)
\end_layout

\begin_layout Standard
t = linspace(t0,T,M+1)
\end_layout

\begin_layout Standard
h=(T-t0)/M
\end_layout

\begin_layout Standard
x(1)=x0;
\end_layout

\begin_layout Standard
y(1)=y0;
\end_layout

\begin_layout Standard
for i=1:M
\end_layout

\begin_layout Standard
x(i+1)=x(i)+hx(ax(i)+by(i));
\end_layout

\begin_layout Standard
y(i+1)=y(i)+hx(cx(i)+dy(i));
\end_layout

\begin_layout Standard
endfor
\end_layout

\begin_layout Standard
endfunction
\end_layout

