\begin_inset Formula $x\left(2\right)$
\end_inset

con error menor que 
\begin_inset Formula $10^{-3}$
\end_inset

.
\begin_inset Newline newline
\end_inset

Calcular m para conseguir esto.
\end_layout

\begin_layout Subsubsection
Con Euler
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i},x_{i}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\Phi\left(t,x,h\right)=f\left(t,x\right)=sen\left(x\right)+2t$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h\left[f\left(t,x\right)\right]+h\tau_{t}\left(h\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h\left[x'\left(t\right)\right]+\frac{h^{2}}{2}x''\left(\theta_{t,h}\right)$
\end_inset

 con 
\begin_inset Formula $\theta_{t,h}\leq\left(t,t+h\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\tau_{t}\left(h\right)=\frac{h}{2}x''\left(\theta_{t,h}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x''\left(t\right)=\frac{d}{dt}\left(x'\left(t\right)\right)=\frac{d}{dt}\left(f\left(t,x\left(t\right)\right)\right)=\frac{d}{dt}\left(sen\left(x\left(t\right)\right)+2t\right)=x'\left(t\right)cos\left(x\left(t\right)\right)+2=\left(sen\left(x\left(t\right)\right)+2t\right)cos\left(x\left(t\right)\right)+2$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $t_{0}\leq t\leq T\left(0\leq t\leq2\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|x''\left(t\right)\right|\leq\left(\left|sen\left(x\left(t\right)\right)\right|+2\left|t\right|\right)\left|cos\left(x\left(t\right)\right)\right|+2=7$
\end_inset

 
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|\tau_{i}\right|=\left|\tau_{ti}\left(h\right)\right|=\left|\frac{h}{2}x''\left(\theta_{t_{i}h}\right)\right|$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $t_{0}\leq t_{i}<\theta_{t_{i}h}<t_{i}+h\leq t_{max}h=T\leq\frac{7}{2}h$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\tau_{max}=max\left\{ \left|\tau_{i}\right|\right\} \leq\frac{7}{2}h$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|\Phi\left(t,x_{1},h\right)-\Phi\left(t,x_{2},h\right)\right|=\left|\left(sen\left(x_{1}\right)+2t\right)-\left(sen\left(x_{2}\right)+2t\right)\right|=\left|sen\left(x_{1}\right)-sen\left(x_{2}\right)\right|\leq\left|x_{1}-x_{2}\right|$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|x\left(2\right)-x_{m}\right|\leq\left(\frac{7}{2}h\right)\left(e^{1\left(2-0\right)}-1\right)=\frac{7}{2}h\left(e^{2}-1\right)\leq28h=28\left(\frac{2-0}{m}\right)=\frac{56}{m}<10^{-3}$
\end_inset


\end_layout

\begin_layout Subsubsection
Con Taylor de orden 2
\end_layout

\begin_layout Standard
\begin_inset Formula $\Phi\left(t,x,h\right)=f\left(t,x\right)+\frac{h}{2}\left[f_{t}\left(t,x\right)+f_{x}\left(t,x\right)\right]$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h\Phi\left(t,x,h\right)+h\tau_{t}\left(h\right)=x\left(t\right)+hf\left(t,x\right)+\frac{h^{2}}{2}\left[f_{t}\left(t,x\right)+f_{x}\left(t,x\right)f\left(t,x\right)\right]+h\tau_{t}\left(h\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+hx'\left(t\right)+\frac{h^{2}}{2}x''\left(t\right)+\frac{h^{3}}{6}x'''\left(\theta_{t,h}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\tau_{t}\left(h\right)=\frac{h^{2}}{6}x'''\left(\theta_{t,h}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x''\left(t\right)=x'\left(t\right)cos\left(x\left(t\right)\right)+2$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x'''\left(t\right)=x''\left(t\right)cos\left(x\left(t\right)\right)-\left(x'\left(t\right)\right)^{2}sen\left(x\left(t\right)\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $0\leq t\leq2$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|x'''\left(t\right)\right|\leq\left|x''\left(t\right)\right|+\left|x'\left(t\right)\right|^{2}\leq7+\left|sen\left(x\right)+2t\right|^{2}\leq5+16=32$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|\tau_{t_{i}}\left(h\right)\right|\leq\frac{h^{2}}{6}32\leq6h^{2}$
\end_inset


\begin_inset Newline newline
\end_inset

constante de Lipschitz para 
\begin_inset Formula $\Phi$
\end_inset

 en x
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\Phi\left(t,x,h\right)=sen\left(x\right)+2t+\frac{h}{2}\left[2+cos\left(x\right)\left(sen\left(x\right)+t\right)\right]$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|\Phi\left(t,x_{1},h\right)-\Phi\left(t,x_{2},h\right)\right|=\left|\left(sen\left(x_{1}\right)+2t+\frac{h}{2}\left[2+cos\left(x_{1}\right)\left(sen\left(x_{1}\right)+t\right)\right]\right)-\left(sen\left(x_{2}\right)+2t+\frac{h}{2}\left[2+cos\left(x_{2}\right)\left(sen\left(x_{2}\right)+t\right)\right]\right)\right|$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $=\left|\left(sen\left(x_{1}\right)-sen\left(x_{2}\right)\right)+\frac{h}{2}2t\left(cos\left(x_{1}\right)-cos\left(x_{2}\right)\right)+\frac{h}{2}\left(cos\left(x_{1}\right)sen\left(x_{1}\right)-cos\left(x_{2}\right)sen\left(x_{2}\right)\right)\right|\leq$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|x_{1}-x_{2}\right|+ht\left|x_{1}-x_{2}\right|+\frac{h}{4}\left|sen\left(2x_{1}\right)-sen\left(2x_{2}\right)\right|=\left(1+ht+\frac{h}{2}\right)\left|x_{1}-x_{2}\right|\leq\left(1+\frac{5}{2}h\right)\left|x_{1}-x_{2}\right|$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\left|x\left(2\right)-x_{m}\right|=\left(\frac{\tau_{max}}{K}\right)\left(e^{K\left(2-0\right)}-1\right)\leq\frac{gh^{2}}{\frac{7}{2}}\left(e^{\frac{7}{2}2}-1\right)\leq\frac{12}{7}h^{2}e^{7}<10^{-3}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\frac{2}{m}=h<\frac{1}{\sqrt{2}}10^{-2}e^{-4}$
\end_inset


\begin_inset Newline newline
\end_inset

Entonces 
\begin_inset Formula $32000<m$
\end_inset


\end_layout

\begin_layout Subsection
