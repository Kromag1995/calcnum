\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\rightarrow g$
\end_inset

 es creciente y 
\begin_inset Formula $g\left(0\right)=\sqrt{2}\epsilon\left[0,\infty\right)\rightarrow g\left(I\right)\subset I$
\end_inset

 
\end_layout

\begin_layout Standard
El metodo iterativo converge 
\begin_inset Formula $\left|g'\left(x\right)\right|<1$
\end_inset

 y 
\begin_inset Formula $g\left(I\right)\subset I$
\end_inset


\end_layout

\begin_layout Standard
