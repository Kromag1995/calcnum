\begin_layout Subsection
Eliminacion Gaussiana
\end_layout

\begin_layout Standard
\begin_inset Formula $A\epsilon\Re^{nxn},\,b\epsilon\Re^{nxn}$
\end_inset

 
\end_layout

\begin_layout Standard
