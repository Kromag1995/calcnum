\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $2>\frac{1}{\alpha}>0$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $2>\frac{1}{\alpha}$
\end_inset


\end_layout

\begin_layout Standard
