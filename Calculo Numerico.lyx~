\end_layout

\begin_layout Subsubsection
Metodos Implicitos (de un paso)
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}=x_{i}+h\Phi\left(t_{i+1},x_{i+1},k\right)$
\end_inset


\end_layout

\begin_layout Subsubsection
Metodo de Euler Implicito (Eurler-Bacnord)
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i},x_{i}\right)$
\end_inset

 Euler normal
\end_layout

\begin_layout Standard
Esto hace una proximacion con la derivada.
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i+1},x_{i+1}\right)$
\end_inset

 Euler Implicito
\end_layout

\begin_layout Standard
Para acotar 
\begin_inset Formula $\left|\tau_{t}\left(k\right)\right|$
\end_inset

en un metodo de orden k hay que acptar 
\begin_inset Formula $\left|x^{\left(j\right)}\left(t\right)\right|$
\end_inset

 en 
\begin_inset Formula $\left[t_{0},T\right]$
\end_inset


\end_layout

\begin_layout Paragraph
Ejemplo
\end_layout

\begin_layout Standard
\begin_inset Formula $\begin{cases}
x'=-tx^{3}\\
x\left(0\right)=1
\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
Para el PVI se requiere aproximar x en 
\begin_inset Formula $\left[0,T\right]$
\end_inset

 utilizando el metodo de euler implicito acotar el error en funcion de T
\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i+1},x_{i+1}\right)=x_{i}-ht_{i+1}x_{i+1}^{3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x_{i+1}+ht_{i+1}x_{i+1}^{3}=x_{i}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+hf\left(t+h,x\left(t+h\right)\right)+h\tau_{t}\left(h\right)$
\end_inset


\end_layout

\begin_layout Standard
sumo y resto 
\begin_inset Formula $hx\text{'\left(t\right)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x\left(t\right)+hx\text{'\left(t\right)}+h\left(f\left(t+h,x\left(t+h\right)\right)-x\text{'\left(t\right)}+\tau_{t}\left(h\right)\right)$
\end_inset


\end_layout

\begin_layout Standard
Si a 
\begin_inset Formula $x\left(t\right)$
\end_inset

 le aplico Taylor y valor medio queda lo siguiente:
\end_layout

\begin_layout Standard
\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+hx\text{'\left(t\right)}+\frac{h^{2}}{L}x''\left(\Theta_{t,h}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $h\left(f\left(t+h,x\left(t+h\right)\right)-x\text{'\left(t\right)}+\tau_{t}\left(h\right)\right)=\frac{h^{2}}{L}x''\left(\Theta_{t,h}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\tau_{t}\left(h\right)+h^{2}x''\left(M_{t},h\right)=\frac{h^{2}}{2}x''\left(\Theta_{t,h}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\tau_{t}\left(h\right)=h^{2}\left(\frac{x''\left(\Theta_{t,h}\right)}{2}-x''\left(M_{t},h\right)\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\tau_{t}\left(h\right)\right|\leq h^{2}\left(\frac{\left|x''\left(\Theta_{t,h}\right)\right|}{2}+\left|x''\left(M_{t},h\right)\right|\right)$
\end_inset


\end_layout

\begin_layout Standard
Acotemos 
\begin_inset Formula $x''\left(t\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x\left(t\right)=\frac{d\left(-tx^{3}\right)}{dt}=-x^{3}\left(t\right)-3tx^{2}\left(t\right)x'\left(t\right)=-x^{3}\left(t\right)+3t^{2}x^{5}$
\end_inset


\end_layout

\begin_layout Standard
Podemos ver que el modulo de la solucion es estrictamente decreciente para
 t mayo que cero.
\end_layout

\begin_layout Standard
\begin_inset Formula $\left|x''\left(t\right)\right|=\left|-x^{3}\left(t\right)+3t^{2}x^{5}\left(t\right)\right|\leq\left|x\left(t\right)\right|^{3}+3\left|t\right|^{2}\left|x\left(t\right)\right|^{5}\leq\left(1+3\right)T^{2}$
\end_inset


\end_layout

\begin_layout Standard
Aca use que 
\begin_inset Formula $x\left(t\right)$
\end_inset

siempre es decreciente y empieza en 1, entonces su maximo valor es 1.
\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|\tau_{t}\left(h\right)\right|\leq h^{2}\left(\frac{\left|x''\left(\Theta_{t,h}\right)\right|}{2}+\left|x''\left(M_{t},h\right)\right|\right)\right|\leq h^{2}\left(\frac{\left(1+3T^{2}\right)}{2}+\left(1+3T^{2}\right)\right)=\frac{3h\left(1+3T^{2}\right)}{2}$
\end_inset

 si 
\begin_inset Formula $t,t+h\epsilon\left[0,1\right]$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\tau_{i}\left(h\right)\right|\leq\frac{3h\left(1+3T^{2}\right)}{2}$
\end_inset


\end_layout

\begin_layout Subsection
Runge-Kutta
\end_layout

\begin_layout Standard
Los metodos de Taylor requieren calcular derivadas y eso termina siendo
 muy cuentoso.
\end_layout

\begin_layout Paragraph
Idea:
\end_layout

\begin_layout Standard
Con Euler: 
\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+\intop_{t}^{t+h}x'\left(s\right)ds$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i},x_{i}\right)$
\end_inset


\end_layout

\begin_layout Standard
Por ahi es mejor si evaluo en varios 
\begin_inset Formula $\left(\theta_{i},\gamma_{i}\right)$
\end_inset

, con 
\begin_inset Formula $\left(\theta_{i},\gamma_{i}\right)$
\end_inset

 cerca de 
\begin_inset Formula $\left(t_{i},x_{i}\right)$
\end_inset

.
\end_layout

\begin_layout Standard
Tomar 
\begin_inset Formula $\Phi\left(t_{i},x_{i},h\right)=A_{1}f\left(\theta_{1},\gamma_{1}\right)+\ldots+A_{m}f\left(\theta_{m},\gamma_{m}\right)$
\end_inset

, con 
\begin_inset Formula $\left(\theta_{i},\gamma_{i}\right)$
\end_inset

 cerca de 
\begin_inset Formula $\left(t_{i},x_{i}\right)$
\end_inset


\end_layout

\begin_layout Paragraph
Prop:
\end_layout

\begin_layout Standard
\begin_inset Formula $\mathop{\mathop{\sum_{i=1}^{m}}A_{i}=1}$
\end_inset


\end_layout

\begin_layout Paragraph
Ejemplo:
\end_layout

\begin_layout Enumerate
Orden 2: 
\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i}+\frac{h}{2},x_{i}+\frac{h}{2}f\left(t_{i},x_{i}\right)\right)$
\end_inset


\end_layout

\begin_layout Enumerate
Heum: 
\begin_inset Formula $x_{i+1}=x_{i}+\frac{h}{2}\left(f\left(t_{i},x_{i}\right)+f\left(t_{i}+h,x_{i}+hf\left(t_{i},x_{i}\right)\right)\right)$
\end_inset


\end_layout

\begin_layout Enumerate
Orden 4: 
\begin_inset Formula $x_{i+1}=x_{i}+\frac{h}{6}\left(K_{1}+2K_{2}+2K_{3}+K_{4}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $K_{1}=f\left(t_{i},x_{i}\right)$
\end_inset

, 
\begin_inset Formula $K_{2}=f\left(t_{i}+\frac{h}{2},x_{i}+\frac{h}{2}K_{1}\right)$
\end_inset

 , 
\begin_inset Formula $K_{3}=f\left(t_{i}+\frac{h}{2},x_{i}+hK_{2}\right)$
\end_inset

, 
\begin_inset Formula $K_{4}=f\left(t_{i}+\frac{h}{2},x_{i}+hK_{3}\right)$
\end_inset


\end_layout

\begin_layout Section
