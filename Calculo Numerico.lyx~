\end_inset


\end_layout

\begin_layout Subsubsection
Comparacion de numeros de condicion
\end_layout

\begin_layout Standard
Usar comparacion de normas
\end_layout

\begin_layout Standard
Ejemplo 2-
\begin_inset Formula $\infty$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula $Cond_{2}\left(A\right)=\left|\left|A\right|\right|_{2}\left|\left|A^{-1}\right|\right|_{2}\leq\left(\sqrt{m}\left|\left|A\right|\right|_{\infty}\right)\left(\sqrt{m}\left|\left|A^{-1}\right|\right|_{\infty}\right)=m\left|\left|A\right|\right|_{\infty}\left|\left|A^{-1}\right|\right|_{\infty}=mcond_{\infty}\left(A\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $Cond_{2}\left(A\right)\geq\left(\frac{\left|\left|A\right|\right|_{\infty}}{\sqrt{m}}\right)\left(\frac{\left|\left|A\right|\right|_{\infty}}{\sqrt{m}}\right)=\frac{1}{m}Cond_{\infty}\left(A\right)$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Teorema: 
\series default
Si 
\begin_inset Formula $A\in\mathbb{R}^{nxn}$
\end_inset

y es inversible, entonces:
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{1}{cond_{\left|\left|\right|\right|}\left(A\right)}=inf\left\{ \frac{\left|\left|A-B\right|\right|}{\left|\left|A\right|\right|},B\;singular\right\} $
\end_inset


\end_layout

\begin_layout Standard
Aplicacion: A es mlaa proparando errores , tomando matrices B adecuadas
 ve que: 
\begin_inset Formula $\frac{1}{cond\left(A\right)}\leq\left[Algo\ chiquito\right]$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Ejemplo
\series default
:
\end_layout

\begin_layout Standard
\begin_inset Formula $A\left(\epsilon\right)=\left(\begin{array}{ccc}
1 & \epsilon & 2\epsilon\\
4 & 4 & 4\\
1 & 0 & 0
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
1)Ver que 
\begin_inset Formula $A\left(\epsilon\right)$
\end_inset

 es no singular para 
\begin_inset Formula $\epsilon>0$
\end_inset


\end_layout

\begin_layout Standard
2)Estimar 
\begin_inset Formula $cond_{\infty}\left(A\left(\epsilon\right)\right)$
\end_inset

para 
\begin_inset Formula $\epsilon$
\end_inset

 tendiendo a 0
\end_layout

\begin_layout Standard
1)
\begin_inset Formula $det\left(A\left(\epsilon\right)\right)=1\left(-1\right)^{3+1}\left|\begin{array}{cc}
\epsilon & 2\epsilon\\
4 & 4
\end{array}\right|=-4\epsilon\neq0$
\end_inset


\end_layout

\begin_layout Standard
2)
\begin_inset Formula $\frac{1}{cond_{\infty}\left(A\left(\epsilon\right)\right)}=inf\left\{ \frac{\left|\left|A\left(\epsilon\right)-B\right|\right|_{\infty}}{\left|\left|A\left(\epsilon\right)\right|\right|_{\infty}}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\left(\epsilon\right)-B\right|\right|_{\infty}=\left|\left|\begin{array}{ccc}
0 & \epsilon & 2\epsilon\\
0 & 0 & 0\\
0 & 0 & 0
\end{array}\right|\right|_{\infty}=max_{1\leq i\leq3}\left(\sum_{j=1}^{3}\left|\left(A\left(\epsilon\right)-B\right)_{ij}\right|\right)=3\epsilon$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\left(\epsilon\right)\right|\right|_{\infty}=\left|\left|\begin{array}{ccc}
1 & \epsilon & 2\epsilon\\
4 & 4 & 4\\
1 & 0 & 0
\end{array}\right|\right|_{\infty}=12$
\end_inset

 si 
\begin_inset Formula $\epsilon$
\end_inset

 es chiquito.
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{1}{cond_{\infty\left(A\left(\epsilon\right)\right)}}=inf\left\{ \right\} \leq\frac{\left|\left|A\left(\epsilon\right)-B\right|\right|_{\infty}}{\left|\left|A\left(\epsilon\right)\right|\right|_{\infty}}=\frac{\epsilon}{4}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{4}{\epsilon}<cond_{\infty}\left(A\left(\epsilon\right)\right),$
\end_inset

para todo 
\begin_inset Formula $\epsilon<1$
\end_inset


\end_layout

\begin_layout Standard
Entonces 
\begin_inset Formula $cond_{\infty}\left(A\left(\epsilon\right)\right)$
\end_inset

 tiende a 
\begin_inset Formula $\infty$
\end_inset


\end_layout

\begin_layout Standard

\series bold
Ejercicio 2: 
\series default
Sea 
\begin_inset Formula $A\epsilon\mathbb{R}^{2nx2n}$
\end_inset

dada por :
\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{cccc}
\left(\begin{array}{cc}
\frac{1}{n} & 1\\
0 & 1
\end{array}\right) & 0 & 0 & 0\\
0 & \left(\begin{array}{cc}
\frac{2}{n} & 2\\
0 & 2
\end{array}\right) & 0 & 0\\
0 & 0 & \left(\begin{array}{cc}
\frac{3}{m} & n\\
0 & n
\end{array}\right) & 0\\
0 & 0 & 0 & \left(\begin{array}{cc}
\frac{1}{n} & n\\
0 & n
\end{array}\right)
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
1) Ver que A no es singular
\end_layout

\begin_layout Standard
2) Ver que 
\begin_inset Formula $cond_{\infty}\left(A\right)\geq n^{2}$
\end_inset


\end_layout

\begin_layout Standard
3) Ver que 
\begin_inset Formula $cond_{2}\left(A\right)$
\end_inset

 tiende a 
\begin_inset Formula $\infty$
\end_inset


\end_layout

\begin_layout Standard
1)
\begin_inset Formula $det\left(A\right)=\left(\Pi_{i=1}^{n}\frac{i}{n}\right)\left(\Pi_{i=1}^{n}i\right)=\frac{\left(n!\right)^{2}}{n^{n}}\neq0$
\end_inset


\end_layout

\begin_layout Standard
Si una matriz es traingular 
\begin_inset Formula $det\left(A\right)=\Pi_{i}\theta_{ij}$
\end_inset

, entonces A no es singular
\end_layout

\begin_layout Standard
2) Tomo B singular parecida a A
\end_layout

\begin_layout Standard
\begin_inset Formula $B=\left(\begin{array}{ccccc}
\left(\begin{array}{cc}
\frac{1}{n} & 1\\
0 & 1
\end{array}\right) & 0 & 0 & 0 & 0\\
0 & \ddots & 0 & 0 & 0\\
0 & 0 & \left(\begin{array}{cc}
0 & i\\
0 & i
\end{array}\right) & 0 & 0\\
0 & 0 & 0 & \ddots & 0\\
0 & 0 & 0 & 0 & \left(\begin{array}{cc}
\frac{1}{n} & n\\
0 & n
\end{array}\right)
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Standard
B es igual a A menos en la posicion i-esima.
 
\begin_inset Formula $B_{i}$
\end_inset

 es singular porque las filas 2i-1, 2i son iguales.
\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\left(\epsilon\right)-B\right|\right|_{\infty}=\left|\left|\begin{array}{ccccc}
0 & 0 & 0 & 0 & 0\\
0 & \ddots & 0 & 0 & 0\\
0 & 0 & \left(\begin{array}{cc}
\frac{i}{n} & 0\\
0 & 0
\end{array}\right) & 0 & 0\\
0 & 0 & 0 & \ddots & 0\\
0 & 0 & 0 & 0 & 0
\end{array}\right|\right|_{\infty}=\frac{i}{n}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left|\left|A\right|\right|_{\infty}=\frac{n}{n}+n=n+1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{1}{cond_{\infty1}\left(A\right)}=inf\left\{ \right\} \leq\frac{\left|\left|A\left(\epsilon\right)-B\right|\right|_{\infty}}{\left|\left|A\left(\epsilon\right)\right|\right|_{\infty}}=\frac{i}{n\left(n+1\right)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $cond_{\infty1}\left(A\right)\geq\frac{n^{2}+n}{i}$
\end_inset

, puedo tomar 
\begin_inset Formula $i=1$
\end_inset

,
\begin_inset Formula $cond_{\infty}\left(A\right)\geq n^{2}+n\geq n^{2}$
\end_inset


\end_layout

\begin_layout Standard
