\series medium
: Hallar una discratizacion de la derivada que requiera solo saltar hacia
 adelante y que tenga error 
\begin_inset Formula $O\left(h^{2}\right)$
\end_inset


\end_layout

\begin_layout Standard
Hagamos cuentas con 
\begin_inset Formula $u\left(x\right),u\left(x+h\right),u\left(x+2h\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u'\left(x\right)=\left[\frac{u\left(x+h\right)-u\left(x\right)}{h}\right]+O\left(h\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $hu'\left(x\right)=u\left(x+h\right)-u\left(x\right)+O\left(h^{2}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $hu'\left(x\right)+O\left(h^{2}\right)=1*u\left(x+h\right)+\left(-1\right)*u\left(x\right)$
\end_inset


\end_layout

\begin_layout Standard
Busco A, B, C 
\begin_inset Formula $\epsilon R$
\end_inset

 independientes de x y de h tales que:
\end_layout

\begin_layout Standard
\begin_inset Formula $Au\left(x\right)+Bu\left(x+h\right)+Cu\left(x+2h\right)=hu'\left(x\right)+O\left(h^{3}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u\left(x+h\right)=u\left(x\right)+hu'\left(x\right)+\frac{1}{2}h^{2}u''\left(x\right)+\frac{1}{6}u^{3}u'''\left(\theta_{1}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u\left(x+2h\right)=u\left(x\right)+2hu'\left(x\right)+2h^{2}u''\left(x\right)+\frac{4}{3}u^{3}u'''\left(\theta_{2}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $Au\left(x\right)+Bu\left(x+h\right)+Cu\left(x+2h\right)=\left(A+B+C\right)u\left(x\right)+\left(B+2C\right)hu'\left(x\right)+\left(\frac{B}{2}+2C\right)h^{2}u''\left(x\right)+\left(\frac{1}{6}Bu'''\left(\theta_{1}\right)+\frac{4}{3}Cu'''\left(\theta_{2}\right)\right)h^{3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left\{ \begin{array}{c}
A+B+C=0\\
B+2C=1\\
\frac{B}{2}+2C=0
\end{array}\right.$
\end_inset


\end_layout

\begin_layout Standard
Entonces 
\begin_inset Formula $B=2$
\end_inset

, 
\begin_inset Formula $C=-\frac{1}{2}$
\end_inset

 y 
\begin_inset Formula $A=-\frac{3}{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $-\frac{3}{2}u\left(x\right)+2\left(x+h\right)-\frac{1}{2}u\left(x++2h\right)=hu'\left(x\right)+O\left(h^{3}\right)$
\end_inset

 
\end_layout

\begin_layout Standard
Si 
\begin_inset Formula $\left|u'''\left(\theta\right)\right|\leq K$
\end_inset

 para 
\begin_inset Formula $\theta\epsilon I=\left[a,b\right]$
\end_inset

 y 
\begin_inset Formula $x,x+h,x+2h\epsilon I$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{-\frac{3}{2}u\left(x\right)+2\left(x+h\right)-\frac{1}{2}u\left(x++2h\right)}{h}=u'\left(x\right)+O\left(h^{2}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $u'\left(x_{0}\right)\sim\frac{-\frac{3}{2}u\left(x\right)+2\left(x+h\right)-\frac{1}{2}u\left(x++2h\right)}{h}$
\end_inset


\end_layout

