\begin_layout Subsection
Ecuaciones diferenciales
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
x'\left(t\right) & =f\left(t,x\left(t\right)\right)\\
 & X\left(t_{0}\right)=X_{0}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Metodo de Euler
\end_layout

\begin_layout Standard
Es un taylor a primer orden
\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+hx'\left(t\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+hf\left(t,x\right)$
\end_inset


\begin_inset Newline newline
\end_inset

Entonces:
\begin_inset Newline newline
\end_inset


\begin_inset Formula $x_{0}$
\end_inset

Dato
\begin_inset Newline newline
\end_inset


\begin_inset Formula $x_{i+1}=x_{i}+hf\left(t_{i}x_{i}\right)$
\end_inset


\end_layout

\begin_layout Subsubsection
Taylor de orden 2:
\end_layout

\begin_layout Standard
\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h.x'\left(t\right)+h^{2}.x''\left(t\right)/2$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h.f\left(t,x\right)+h^{2}??$
\end_inset


\begin_inset Newline newline
\end_inset

Pero como 
\begin_inset Formula $\begin{cases}
x'\left(t\right)=f\left(t,x\left(t\right)\right)\\
x''\left(t\right)=f_{t}\left(t,x\left(t\right)\right)+f_{x}\left(t,x\left(t\right)\right)f\left(t,x\left(t\right)\right)
\end{cases}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+h.f\left(t,x\right)+h^{2}\left(f_{t}\left(t,x\left(t\right)\right)+f_{x}\left(t,x\left(t\right)\right)f\left(t,x\left(t\right)\right)\right)/2$
\end_inset

 
\end_layout

\begin_layout Standard
Metodo de Taylor de orden 2
\end_layout

\begin_layout Standard
Aplicado a cada coordenada de un vector x.
\end_layout

\begin_layout Standard
Ejemplo:
\begin_inset Formula $\begin{cases}
x\left(t\right)=t*sen\left(x\left(t\right)\right)\\
x\left(0\right)=1
\end{cases}$
\end_inset


\begin_inset Newline newline
\end_inset

Metodo de Euler
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\begin{cases}
x_{0}=1\\
x_{i+1}=x_{i}+h*t_{i}*sen\left(x_{i}\left(t\right)\right)
\end{cases}$
\end_inset


\begin_inset Newline newline
\end_inset

Metodo de Taylor orden 2
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\begin{cases}
x_{0}=1\\
x_{i+1}=x_{i}+h*t_{i}*sen\left(x_{i}\right)+\frac{h^{2}}{2}\left[sen\left(x_{i}\right)+t_{i}^{2}*cos\left(x_{i}\right)*sen\left(x_{i}\right)\right]
\end{cases}$
\end_inset


\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Subsubsection
Metodo de Taylor orden k:
\end_layout

\begin_layout Standard
Idea: 
\begin_inset Formula $x\left(t+h\right)=x\left(t\right)+x'\left(t\right)h+...+\frac{h^{k}}{k!}x^{\left(k\right)}\left(t\right)$
\end_inset


\end_layout

\begin_layout Subsection
Octave:
\end_layout

\begin_layout Standard
function [x,y]=Euler(x0,xf,f,y0,N)
\begin_inset Newline newline
\end_inset

x(i)=x0
\begin_inset Newline newline
\end_inset

y(i)=y0
\begin_inset Newline newline
\end_inset

h=(tf-t0)/N
\begin_inset Newline newline
\end_inset

for i=2:(N+1)
\end_layout

\begin_layout Standard
x(i)=x(i-1)+h
\end_layout

\begin_layout Standard
y(i)=y(i-1)+hf(x(i-1),y(i-1))
\begin_inset Newline newline
\end_inset

end for
\begin_inset Newline newline
\end_inset

plot(x,y) #hold on, pega el grafico uno arriba del otro
\end_layout

